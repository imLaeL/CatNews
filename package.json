{
  "name": "catnews",
  "type": "module",
  "version": "1.0.0",
  "description": "Para fazer download do repositório usando o git, basta cloná-lo da seguinte forma: git clone https://github.com/imLaeL/CatNews",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "db:reload": "npm run db:drop && npm run db:load",
    "db:load": "node src/database/migration.js && node src/database/seeders.js",
    "db:drop": "rm src/database/database.sqlite"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imLaeL/CatNews.git"
  },
  "author": "Shannon Brunna, Kayck Jesse, Janaína Vitória e Isaque Lael",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/imLaeL/CatNews/issues"
  },
  "homepage": "https://github.com/imLaeL/CatNews#readme",
  "dependencies": {
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.22",
    "path": "^0.12.7",
    "sequelize": "^6.31.1",
    "sqlite3": "^5.1.6",
    "uuid": "^9.0.0"
  }
}
